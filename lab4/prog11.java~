import java.util.*;
import java.lang.Math;
class eleventh
{
	public static void main(String ags[])
	{
	Scanner sc=new Scanner(System.in);
	int x,nch;
	System.out.println("enter your current balance");
	x=sc.nextInt();
	System.out.println("enter no of cheques used");
	nch=sc.nextInt();
	int ch=25;
	if(x<2000)
	ch+=100;
	if(nch<20)
	{
		ch+=(nch*25);
	}
	else if(nch<40)
	{
		ch+=(19*25+(nch-19)*20);
	}
	else if(nch<60)
	ch+=(19*25+20*20+(nch-39)*15);
	else
	ch+=(19*25+20*20+20*15+(nch-59)*10);
	
	System.out.println("your bank service fees is= "+ch);
	
	
	}
}
